pipeline {
  agent any
  
  stages {
    stage('Clone Repo') {
      steps {
        echo "Cloning the project"
      }
    }

    stage('Build') {
      steps {
        echo "Building the project"
      }
    }

    stage('Test') {
      steps {
        echo "Testing the project"
      }
    }

    
  }
}
